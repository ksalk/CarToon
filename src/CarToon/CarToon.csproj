<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net8.0;netstandard2.0</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageId>CarToon</PackageId>
    <Title>CarToon</Title>
    <Version>0.1.0</Version>
    <Authors>ksalk</Authors>
    <Description>TOON (TOON Object Notation) serialization library for .NET</Description>
    <PackageProjectUrl>https://github.com/ksalk/CarToon</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/ksalk/CarToon</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>toon;serialization;object-notation;nuget</PackageTags>
  </PropertyGroup>

</Project>
